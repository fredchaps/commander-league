:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: Inter, ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont,
		'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji',
		'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji';
	--theme-font-family-heading: Inter, ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont,
		'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji',
		'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji';
	--theme-font-color-base: 0 0 0;
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 9999px;
	--theme-rounded-container: 8px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: var(--color-surface-600);
	--on-secondary: var(--color-surface-500);
	--on-tertiary: var(--color-surface-700);
	--on-success: var(--color-surface-500);
	--on-warning: var(--color-surface-500);
	--on-error: 0 0 0;
	--on-surface: var(--color-primary-400);
	/* =~= Theme Colors  =~= */
	/* primary | #D2BA86 */
	--color-primary-50: 248 245 237; /* ⬅ #f8f5ed */
	--color-primary-100: 246 241 231; /* ⬅ #f6f1e7 */
	--color-primary-200: 244 238 225; /* ⬅ #f4eee1 */
	--color-primary-300: 237 227 207; /* ⬅ #ede3cf */
	--color-primary-400: 224 207 170; /* ⬅ #e0cfaa */
	--color-primary-500: 210 186 134; /* ⬅ #D2BA86 */
	--color-primary-600: 189 167 121; /* ⬅ #bda779 */
	--color-primary-700: 158 140 101; /* ⬅ #9e8c65 */
	--color-primary-800: 126 112 80; /* ⬅ #7e7050 */
	--color-primary-900: 103 91 66; /* ⬅ #675b42 */
	/* secondary | #859775 */
	--color-secondary-50: 237 239 234; /* ⬅ #edefea */
	--color-secondary-100: 231 234 227; /* ⬅ #e7eae3 */
	--color-secondary-200: 225 229 221; /* ⬅ #e1e5dd */
	--color-secondary-300: 206 213 200; /* ⬅ #ced5c8 */
	--color-secondary-400: 170 182 158; /* ⬅ #aab69e */
	--color-secondary-500: 133 151 117; /* ⬅ #859775 */
	--color-secondary-600: 120 136 105; /* ⬅ #788869 */
	--color-secondary-700: 100 113 88; /* ⬅ #647158 */
	--color-secondary-800: 80 91 70; /* ⬅ #505b46 */
	--color-secondary-900: 65 74 57; /* ⬅ #414a39 */
	/* tertiary | #748C8C */
