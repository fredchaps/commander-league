	--color-tertiary-400: 158 175 175; /* ⬅ #9eafaf */
	--color-tertiary-500: 116 140 140; /* ⬅ #748C8C */
	--color-tertiary-600: 104 126 126; /* ⬅ #687e7e */
	--color-tertiary-700: 87 105 105; /* ⬅ #576969 */
	--color-tertiary-800: 70 84 84; /* ⬅ #465454 */
	--color-tertiary-900: 57 69 69; /* ⬅ #394545 */
